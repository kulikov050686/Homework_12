<Window x:Class="Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:UserControls"
        xmlns:cmd="clr-namespace:Commands"
        xmlns:c="clr-namespace:Converters"
        mc:Ignorable="d"        
        DataContext="{Binding MainWindowVM, Source={StaticResource Locator}}"
        Title="{Binding Title}" 
        Height="850" 
        Width="800">

    <Grid>
        <Frame Content="{Binding CurrentPage, Converter={c:ApplicationPageValueConverter}}"/>
    </Grid>
</Window>
